.container {
  position: relative;
  height: 640rpx;
}


.btn {
  position: fixed;
  top: 50%;
  background-color: #d9d9d9;
  width: 180rpx;
  height: 180rpx;
  border-radius: 100%;
  text-align: center;
  transform: translateY(-50%);
  line-height: 180rpx;
  box-shadow: 0px 0px 45px 1px rgba(0, 0, 0, 0.1);
}

.punchOutResultBlock {
  position: fixed;
  top: 40%;
  text-align: center;
}
